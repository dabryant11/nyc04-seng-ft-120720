<h3>Eric's Chickens </h3>


<%= form_for @item do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :image %>
    <%= f.text_field :image %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :price %>
    <%= f.number_field :price %>
    <%= f.submit %>
<% end %>